.page-leave {
    display:block;
    position:absolute;
    transform:translateX(0%);
    top:0;
    width:100%;
    max-width:640px;
    z-index:9000;
    background: #fff;
}

.page-leave.page-leave-active {
    display:block;
    position:absolute;
    top:0;
    z-index:9000;
    width:100%;
    max-width:640px;
    transform:translateX(-100%);
    transition:transform 0.5s ;
    background: #fff;
}

.page-enter footer,.page-enter header{
    visibility:visible;
}
