/*提示层*/
.alertLayer {
    position: fixed;
    left: 0;
    top: 40%;
    text-align: center;
    width: 100%;
    z-index: 999;
}

.alertBox {
    background: rgba(0, 0, 0, 0.75);
    border-radius: 0.5rem;
    max-width: 7.625rem;
    /*min-width: 4.5rem;*/
    padding: 0.75rem 0.65rem;
    margin: auto;
    font-size: 0.7rem;
    color: #fff;
    -webkit-box-sizing: border-box;
    text-align: center;
}

.alertBox.alertBox-since {
    max-width: 13rem;
}

.alertBox .ic_gou,
.alertBox .ic_sigh-1 {
    display: inline-block;
    background-image: url(../images/panelTips_gou.png);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 1.75rem;
    height: 1.75rem;
}

.alertBox .ic_sigh-2 {
    display: inline-block;
    background: url(../images/panelTips_sigh.png) no-repeat;
    background-repeat: no-repeat;
    background-size: 100%;
    width: 0.5rem;
    height: 1.75rem;
}

.alertBox .ic_cry,
.alertBox .ic_smile {
    display: inline-block;
    background: url(../images/face_cry.png) no-repeat;
    background-size: 100%;
    width: 1.75rem;
    height: 1.75rem;
}

.alertBox .ic_smile {
    background: url(../images/face_smile.png) no-repeat;
    background-size: 100%;
}

.alertBox .alert_msg {
    display: block;
    margin-top: 0.2rem;
    text-align: center;
}

